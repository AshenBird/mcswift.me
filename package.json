  "private": true,
  "scripts": {
    "dev": "cross-env MODE=CSR vite",
    "dev:ssr": "cross-env MODE=SSR esno src/server/main.ts",
    "build:ssr": "npm run build:client && npm run build:server",
    "build:noExternal": "npm run build:client && npm run build:server:noExternal",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/client/entry-server.ts --outDir dist/server",
    "build:server:noExternal": "vite build --config vite.config.noexternal.ts --ssr src/entry-server.ts --outDir dist/server",
    "build:ssg": "cross-env MODE=SSG vite-ssg build --outDir dist/static",
    "serve": "cross-env NODE_ENV=production node server",
    "debug": "node --inspect-brk server"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.6",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/compiler-sfc": "^3.2.20",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "esbuild": "^0.13.12",
    "esno": "^0.10.1",
    "serve-static": "^1.14.1",
    "vite": "^2.6.12",
    "vite-plugin-md": "^0.11.2",
    "vite-ssg": "^0.15.4"
  },
  "dependencies": {
    "@css-render/vue3-ssr": "^0.15.6",
    "@vicons/antd": "^0.11.0",
    "@vicons/fa": "^0.11.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/utils": "^0.1.4",
    "@vue/reactivity": "^3.2.20",
    "@vue/server-renderer": "^3.2.20",
    "@vue/shared": "^3.2.20",
    "@vueuse/head": "^0.6.0",
    "express": "^4.17.1",
    "naive-ui": "^2.19.11",
    "normalize.css": "^8.0.1",
    "vfonts": "^0.1.0",
    "vue": "^3.2.20",
    "vue-router": "^4.0.12"
  }
}
