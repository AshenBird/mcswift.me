{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "jsx": "preserve",
    "allowSyntheticDefaultImports": true,
    "types": [
      "node","vite/client"
    ],
    "typeRoots": ["./node_modules/@types/", "./types"],
  },
  "exclude": ["./server"]
}
